node ('arma') {
  stage('Checkout from Git') {
    checkout scm
  }

  stage('Download latest SFP Objects') {
    bat 'gradlew sfp_objects:downloadMod'
  }

  stage('Download latest SFP Sturko') {
    bat 'gradlew sfp_sturko:downloadMod'
  }

  stage('Run autotest') {
    bat 'gradlew sfp_sturko:runAutotest'
  }

  stage('Copy Log File') {
    bat 'gradlew sfp_sturko:copyLogFile'
  }

  stage('Archive Log File') {
  	archiveArtifacts artifacts: 'sfp_sturko/logs/*.rpt'
  }
}
